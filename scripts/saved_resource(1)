depends=JSON.parse(depends);else return;var dependentElement=$('*[name="'+depends.key+'"]:visible');var dependentValSelector='#'+depends.key;if(dependentElement.attr('type')=='radio')dependentValSelector='input[name="'+depends.key+'"]:checked';var elementId=$(this).attr('id');rules[elementId]={required:{depends:function(){return($(dependentValSelector).val()==depends.value);}}};if(depends.hasOwnProperty('additionalValidators')){for(var i in depends.additionalValidators){rules[elementId][i]=depends.additionalValidators[i];}}});$(this).validate({'errorClass':$(this).attr('data-error-class'),'errorElement':$(this).attr('data-error-element'),highlight:function(element){if(errorWrapperSelector&&errorWrapperClass){$(element).closest(errorWrapperSelector).addClass(errorWrapperClass);}},unhighlight:function(element){if(errorWrapperSelector&&errorWrapperClass){$(element).closest(errorWrapperSelector).removeClass(errorWrapperClass);}},'messages':messages,rules:rules,submitHandler:function(form){if(!$(form).attr('data-form-submitted')){$(form).attr('data-form-submitted','1');if($(form).hasClass('ajax')){$(form).myAjax($(form).attr('data-to-inject'));}else{form.submit();}}}});$(this).attr('data-validation-initialized','1');}else{if(!$(this).hasClass('ajax')){$(this).submit(function(){if(!$(this).attr('data-form-submitted')){$(this).attr('data-form-submitted','1');this.submit();return true;}else{return false;}});}}});}
$.fn.updateUploadAddRemove=function(){if($('.part-optional:hidden',this).length==0){$(".add-upload",this).hide();}else{$(".add-upload",this).show();}
if($('.part-optional:visible',this).length==0){$(".remove-upload",this).hide();}else{$(".remove-upload",this).show();}}
function initUploadWidgets()
{$(".upload-widget input[type=hidden][name^='edit_']").each(function(){$(this).change();});$(".upload-widget").each(function(){$(this).updateUploadAddRemove();});}
function afterAjax()
{initJsValidation();$(window).resize();$('.uf').uniform();$('.tabs').liteTabs();tagCheckboxes();hints();}
function tagCheckboxes()
{$('.tags li').each(function(){var label=$(this).find('label');var checkbox=$(this).find('input[type="checkbox"]');function changeState(){if(checkbox.is(':checked')){label.addClass('active');}
else{label.removeClass('active');}}
changeState()
$(this).find('input[type="checkbox"]').on('change',function(){changeState();});});$('.filter .tag-list').each(function(){var tagList=$(this);var li=$(this).find('li');li.hide().slice(0,6).show();tagList.append('<div><a href="javascript:void(0);" class="btn show-more">Více štítků</a></div>');var more=$(this).find('.show-more');more.click(function(){li.show();more.hide();});});}
function hints()
{$('.hint').qtip({position:{my:'bottom center',at:'top center',adjust:{x:0,y:-10}},style:{tip:{width:15,height:15}},show:{event:'click'},hide:{event:'unfocus'}});}